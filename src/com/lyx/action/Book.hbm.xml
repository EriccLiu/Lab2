<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lyx.action">

<class name="Book" polymorphism="explicit">

	<id name="ISBN">
		<generator class="assigned" ></generator>
	</id>

	<property name="Title" column="Title" ></property>
	<property name="AuthorID" column="AuthorID" ></property>
	<property name="Publisher" column="Publisher" ></property>
	<property name="PublishDate" column="PublishDate" ></property>
	<property name="Price" column="Price" ></property>
	
</class>

</hibernate-mapping>